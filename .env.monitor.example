# Batch queue monitoring configuration
# Used by scripts/monitor-batch-queue.sh
# Copy to .env.monitor and fill in your values

MONITOR_SERVER="root@your-server-ip"
MONITOR_REDIS_CONTAINER="redis-your-service-uuid"
MONITOR_POSTGRES_CONTAINER="postgres-your-service-uuid"
MONITOR_POSTGRES_DB="trigger"
MONITOR_ENV_ID="your-runtime-environment-id"
MONITOR_CONCURRENCY_LIMIT=10
MONITOR_SHARD_COUNT=2
MONITOR_STUCK_THRESHOLD_MIN=15
